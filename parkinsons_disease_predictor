import csv

# Load the dataset
diseases = []   # list of (symptom_list, disease_name)

# CHANGE THIS TO YOUR REAL FILE NAME AND PATH
file_path = "C:/Users/ruhik/Downloads/Healthcare.csv"

with open(file_path) as f:
    reader = csv.DictReader(f)
    for row in reader:
        symptoms = row["Symptoms"].lower().split(",")
        symptoms = [s.strip() for s in symptoms]
        disease = row["Disease"]
        diseases.append((symptoms, disease))

print("Dataset loaded! Total records:", len(diseases))

# User input
user_input = input("Enter symptoms separated by commas:\n").lower()
user_symptoms = [s.strip() for s in user_input.split(",")]

# Matching logic
best_match = None
best_score = 0

for sym_list, dis in diseases:
    score = 0
    for s in user_symptoms:
        if s in sym_list:
            score += 1

    if score > best_score:
        best_score = score
        best_match = dis

# Output
if best_match:
    print("\nPossible Disease:", best_match)
else:
    print("\nNo matching diseaseÂ found.")